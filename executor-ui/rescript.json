{
  "namespace": true,
  "jsx": { "version": 4 },
  "ppx-flags": ["rescript-embed-lang/ppx"],
  "dependencies": ["common", "@rescript/react", "@rescript/webapi", "tilia", "@tilia/react", "rescript-bun"],
  "compiler-flags": [
    "-open RescriptBun",
    "-open RescriptBun.Globals",
    "-open Tilia",
    "-open TiliaReact"
  ],
  "name": "executor-ui",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": {
    "module": "esmodule",
    "in-source": true
  },
  "suffix": ".jsx"
}
