{
  "experimental-features": { "LetUnwrap": true },
  "namespace": true,
  "jsx": { "version": 4 },
  "ppx-flags": ["rescript-embed-lang/ppx"],
  "dependencies": [
    "@rescript/webapi",
    "tilia",
    "@tilia/react",
    "pgtyped-rescript",
    "rescript-bun",
    "executor-ui",
    "common",
    "@rescript/react"
  ],
  "compiler-flags": [
    "-open Tilia",
    "-open RescriptBun",
    "-open RescriptBun.Globals"
  ],
  "name": "executor",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": {
    "module": "esmodule",
    "in-source": true
  },
  "suffix": ".jsx"
}
