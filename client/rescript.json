{
  "jsx": { "version": 4 },
  "dependencies": [
    "@rescript/webapi",
    "@rescript/react",
    "@tilia/tilia",
    "@tilia/react",
    "rescript-bun"
  ],
  "compiler-flags": [
    "-open Tilia",
    "-open TiliaReact",
    "-open RescriptBun",
    "-open RescriptBun.Globals"
  ],
  "uncurried": true,
  "name": "client",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": {
    "module": "esmodule",
    "in-source": true
  },
  "suffix": ".mjs"
}
